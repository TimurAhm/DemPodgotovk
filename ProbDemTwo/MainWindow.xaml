<Window x:Class="ProbDemTwo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProbDemTwo"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="450" MinWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="5*"/>
        </Grid.ColumnDefinitions>
        <Frame Name="FrNav" Grid.Column="1"/>
        <StackPanel Margin="10">
            <Button Name="btSotrudniki" Width="100" Height="20" Content="Сотрудники" Click="btSotrudnikiPageSwichClick"/>
            <Button Name="btZapisi" Width="100" Height="20" Content="Записи" Click="btZapisiPageSwichClick"/>
        </StackPanel>
    </Grid>
</Window>
